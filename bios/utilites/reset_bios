local function format(path)
    local files = apis.fs.list(path)
    for i = 1, #files do
        local file = path..files[i]
        if apis.fs.isDirectory(file) then
            format(file)
        else
            apis.fs.remove(file)
        end
    end
end
if not apis.enterPassword() then
    apis.splash("неверный пароль")
    return
end
local ok = apis.yesno("сброс bios")
if ok then
    format(apis.configspaths)
    computer.shutdown(true)
end