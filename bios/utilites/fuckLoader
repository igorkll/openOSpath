apis.clear()
apis.setText("fuckLoader press insert or center mouse button, or press enter to exit", 1)
local buffer = ""
while true do
    local eventName, uuid, str, char = computer.pullSignal()
    if uuid == apis.keyboard then
        if eventName == "key_down" then
            if char == 28 then
                return
            end
        elseif eventName == "clipboard" then
            buffer = buffer .. str
            if unicode.sub(str, unicode.len(str), unicode.len(str)) ~= "\n" then
                local ok, out = apis.runscript(buffer)
                apis.splash(tostring(out or "unlown"))
                return
            end
        end
    end
end