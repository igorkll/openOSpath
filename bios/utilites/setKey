if not apis.enterPassword() then
    apis.splash("неверный пароль")
    return
end
apis.splash("вас приветствует программа регистрации bios")
local newkey = apis.inputScreen("новый ключь или exit для отмены: ")
if newkey ~= "exit" then
    apis.setBiosKey(newkey)
    apis.splash("ключь БЫЛ сменен")
else
    apis.splash("ключь НЕ был сменен")
end